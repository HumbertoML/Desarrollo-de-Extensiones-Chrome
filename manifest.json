{
    "name": "youtube notificator",
    "version": "1.0",
    "description": "enterate de nuevos videos",
    "manifest_version": 2,
    "author": "Izan",
    "icons": {
        "128": "src/icon128.png"
    },
    "browser_action": {
        "default_title": "youtube notificator",
        "default_icon": "src/icon128.png",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "src/background.html"
    },
    "permissions": [
        "notifications",
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*",
        "cookies",
        "https://www.youtube.com/*"
    ]
}